  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run e2e",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "e2e"
      ],
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}/codewithahsan",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Run replace toolbar test",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "ng-cookbook-recipe:test",
        "--watch"
      ],
      "runtimeExecutable": "nx",
      "cwd": "${workspaceFolder}/codewithahsan",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    }
  ]
}
